@addTagHelper *, Volo.Abp.AspNetCore.Mvc.UI.Bootstrap

@using Ediux.HomeSystem.Web.Renderers

@inject IMarkdownToHtmlRenderer MarkdownRenderer

@model Ediux.HomeSystem.Web.Pages.Components.Pages.PageViewModel

<abp-card>
