@page
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@inject IHtmlLocalizer<HomeSystemResource> L
@inject IPageLayout PageLayout
@model Ediux.HomeSystem.Web.Pages.PersonalCalendar.CreateEventModel
@{
    Layout = null;
}

<abp-dynamic-form abp-model="@Model.CalendarEvent" action="@Url.Page("/PersonalCalendar/CreateEvent")">
    <abp-modal>
        <abp-modal-header title="@L[HomeSystemResource.Menu.PersonalCalendar].Value"></abp-modal-header>
        <abp-modal-body class="input-area">
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@AbpModalButtons.Save | AbpModalButtons.Cancel"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>
@section scripts {   
    
}